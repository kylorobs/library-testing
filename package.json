    "@storybook/addon-links": "^6.1.15",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storysource": "^6.1.15",
    "@storybook/html": "^6.1.15",
    "@types/jest": "24.0.20",
    "babel-loader": "^8.2.2",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "rollup-plugin-node-polyfills": "^0.2.1"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/sass": "^1.3.1",
